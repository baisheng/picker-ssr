    "axios": "^0.16.2",
    "bounding-client-rect": "^1.0.5",
    "bunyan": "^1.8.12",
    "chalk": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "gravatar": "^1.6.0",
    "ioredis": "^3.1.4",
    "js-cookie": "^2.1.4",
    "jwt-decode": "^2.2.0",
