{
  "name": "picker",
  "version": "1.0.0",
  "description": "Picker manager ssr version",
  "author": "baisheng <baisheng@outlook.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=nuxt:*,app nodemon -w server -w nuxt.config.js --exec babel-node server/app.js",
    "dev:pm2": "pm2 start yarn --name=picker -- dev",
    "test": "yarn lint && yarn build:client && ava -verbose --serial",
    "build": "cross-env NODE_ENV=production yarn build:client && yarn build:server",
    "build:client": "nuxt build",
    "build:server": "webpack --config server/webpack.config.babel.js",
    "start": "cross-env NODE_ENV=production node .build/dist/server-app.*.js",
    "start:pm2": "pm2 start yarn --name=picker -- start",
    "generate": "nuxt generate",
    "analyze": "cross-env NODE_ENV=production nuxt build --analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "lint-staged"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "lint-staged": {
    "{client,server,test}/**/*.{js,vue}": [
      "eslint --ext .js,.vue --ignore-path .gitignore"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "^4.3.0",
    "@nuxtjs/component-cache": "^1.1.0",
    "@nuxtjs/proxy": "^1.1.2",
    "awe-dnd": "^0.3.0",
    "axios": "^0.16.2",
    "bezier-easing": "^2.0.3",
    "bounding-client-rect": "^1.0.5",
    "bunyan": "^1.8.12",
    "chalk": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.5",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "global": "^4.3.2",
    "gravatar": "^1.6.0",
    "ioredis": "^3.1.4",
    "js-cookie": "^2.1.4",
    "jwt-decode": "^2.2.0",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-bunyan": "^1.0.0",
    "koa-bunyan-logger": "^2.0.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-connect": "^2.0.0",
    "koa-cookie": "^1.0.0",
    "koa-proxies": "^0.6.1",
    "koa-router": "^7.2.1",
    "koa-session": "^5.5.0",
    "koa-session2": "^2.2.5",
    "moment": "^2.18.1",
    "negotiator": "^0.6.1",
    "nodemon": "^1.12.1",
    "nuxt": "^1.0.0-rc11",
    "popper.js": "^1.12.5",
    "progressbar.js": "^1.0.1",
    "socket.io": "^2.0.3",
    "vee-validate": "^2.0.0-rc.14",
    "vue-awesome": "^2.3.1",
    "vue-class-component": "^5.0.2",
    "vue-click-outside": "^1.0.6",
    "vue-upload-component": "^2.5.0-beta.4",
    "vuex-class": "^0.2.0",
    "xmlify": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^0.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.7.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-minifier": "^3.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "lodash": "^4.17.4",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.3",
    "progress-bar-webpack-plugin": "^1.10.0",
    "sass-loader": "^6.0.6",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">=7.6.0"
  }
}
