{
  "name": "picker",
  "version": "1.0.0",
  "description": "Picker manager ssr version",
  "author": "baisheng <baisheng@outlook.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=nuxt:*, nodemon -w server -w nuxt.config.js --exec node server/app.js",
    "dev:pm2": "pm2 start yarn --name=picker -- dev",
    "test": "yarn lint && yarn build:client && ava -verbose --serial",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production node server/app.js",
    "generate": "nuxt generate",
    "analyze": "cross-env NODE_ENV=production nuxt build --analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
